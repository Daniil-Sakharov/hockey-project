# Database
DATABASE_URL=postgres://hockey:password@localhost:5432/hockey_stats?sslmode=disable

# FHR Registry
FHR_URL=https://registry.fhr.ru
FHR_USERNAME=your_login
FHR_PASSWORD=your_password

# Scraping
SCRAPING_MAX_PLAYERS=1000
SCRAPING_HEADLESS_MODE=true
SCRAPING_TIMEOUT=60s

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Application
APP_ENV=development
SERVICE_NAME=hockey-stats-parser
